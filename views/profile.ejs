                    <div class="dashboard-section-content-side flex-c">
                        <h1 class="section-title">Add Employees</h1>
                        <form action='/profile/addUsers' method="post" id="addUsersForm">
                            
                            <input type="email" name="email" id="employeeEmail" placeholder="Please enter email address" class="credentials_input" required>
                            <input type="text" name="position" id="employeePosition" placeholder="Position" class="credentials_input" required>
                            <div>
                                <label for="employeeIsAdmin">Is Admin?</label>
                            <input type="checkbox" name="isAdmin" id="employeeIsAdmin">
                            </div>
                            <button type="submit" class="primaryBtn" id="addUsersBtn">Add users</button>
                        </form>
                    </div>

                </div>
                
            <% } %>

        </div>

        <div class="menu-container">
            <button class="menu-btns" id="menuHomeBtn"><i class="menu-icons fa-solid fa-house"></i></button>
            <% if((user.userType === "Employee" && user.userData.company != undefined) || (user.userType === "Organization")) { %>
                <button class="menu-btns" id="menuFeedbacksBtn" data-type='<%= user.userType %>' data-admin='<%= (user.userType === "Employee") ? user.userData.isAdmin : false %>'><i class="menu-icons fa-solid fa-comments"></i></button>
            <% } %>
            <% if(user.userType === "Organization" || (user.userType === "Employee" && user.userData.isAdmin) || (user.userType === "Employee" && user.userData.company === undefined)) { %>
                <button class="menu-btns" data-type='<%= user.userType %>' data-admin='<%= (user.userType === "Employee") ? user.userData.isAdmin : false %>' id="menuInvitationsBtn"><i class="menu-icons fa-solid fa-envelope"></i></button>
            <% } %>
            
            <% if(user.userData.isAdmin || user.userType === "Organization") { %> 
                <button class="menu-btns" id="menuAddUsersBtn"><i class="menu-icons fa-solid fa-user-plus"></i></button>
            <% } %>
            <button class="menu-btns" id="menuSignOutBtn"><i class="menu-icons fa-solid fa-right-from-bracket"></i></button>
        </div>
    
    </div>

    
